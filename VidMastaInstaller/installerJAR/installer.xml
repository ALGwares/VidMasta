        <res id="Unix_shortcutSpec.xml" src="installer_Unix_shortcutSpec.xml"/>
        <res id="installer.langsel.img" src="C:\Users\Anthony\workspace\Netbeans\VidMasta\vidmasta\icon48x48.png"/>
